{
  "name": "@neam/neamtime-log-parser",
  "version": "0.3.0",
  "description": "Parses neamtime time tracking logs",
  "main": "build/main/index.js",
  "typings": "build/main/index.d.ts",
  "module": "build/module/index.js",
  "repository": "https://github.com/motin/neamtime-log-parser",
  "license": "UNLICENSED",
  "publishConfig": {
    "registry": "http://localhost:4873/"
  },
  "keywords": [],
  "scripts": {
    "info": "npm-scripts-info",
    "build": "run-s clean && run-p build:*",
    "build:main": "tsc -p tsconfig.json",
    "build:module": "tsc -p tsconfig.module.json",
    "hot-sync:ttbwsd-core": "rsync -r -t -p -o -g -x --stats --delete -l -D build/ ../../products/ttbwsd/ttbwsd-core/node_modules/@neam/neamtime-log-parser/build/",
    "hot-sync:ttbwsd-cli": "rsync -r -t -p -o -g -x --stats --delete -l -D build/ ../../products/ttbwsd/ttbwsd-cli/node_modules/@neam/neamtime-log-parser/build/",
    "postbuild": "run-p hot-sync:*",
    "commit": "git-cz",
    "fix": "run-s fix:*",
    "fix:prettier": "prettier \"src/**/*.ts\" --write",
    "fix:tslint": "tslint -t verbose --fix --project .",
    "lint": "tslint -t verbose --project . && prettier \"src/**/*.ts\" --list-different",
    "test": "run-s build test:*",
    "test:unit": "ava --verbose",
    "posttest:unit": "nyc ava --verbose",
    "dev-test": "run-s build dev-test:*",
    "dev-test:unit": "ava --verbose --serial",
    "postdev-test:unit": "nyc ava --verbose --serial",
    "watch": "run-s clean build:main && run-p \"build:main -- -w\" \"test:unit -- --watch\"",
    "cov": "run-s build test:unit cov:html && opn coverage/index.html",
    "cov:html": "nyc report --reporter=html",
    "cov:send": "nyc report --reporter=lcov > coverage.lcov && codecov",
    "cov:check": "nyc report && nyc check-coverage --lines 100 --functions 100 --branches 100",
    "doc": "run-s doc:html && opn build/docs/index.html",
    "doc:html": "typedoc src/ --target ES6 --mode file --out build/docs",
    "doc:json": "typedoc src/ --target ES6 --mode file --json build/docs/typedoc.json",
    "doc:publish": "gh-pages -m \"[ci skip] Updates\" -d build/docs",
    "version-major": "git stash && run-s prepare && standard-version --release-as major",
    "version-minor": "git stash && run-s prepare && standard-version --release-as minor",
    "version-patch": "git stash && run-s prepare && standard-version --release-as patch",
    "reset": "git clean -dfx && git stash && npm i",
    "clean": "trash build test",
    "all": "run-s reset test cov:check doc:html",
    "prepare": "run-s lint build",
    "prepare-release": "run-s all version doc:publish"
  },
  "scripts-info": {
    "info": "Display information about the package scripts",
    "build": "Clean and rebuild the project",
    "fix": "Try to automatically fix any linting problems",
    "test": "Lint and unit test the project",
    "watch": "Watch and rebuild the project on save, then rerun relevant tests",
    "cov": "Rebuild, run tests, then create and open the coverage report",
    "doc": "Generate HTML API documentation and open it in a browser",
    "doc:json": "Generate API documentation in typedoc JSON format",
    "version": "Bump package.json version, update CHANGELOG.md, tag release",
    "reset": "Delete all untracked files and reset the repo to the last commit",
    "prepare-release": "One-step: clean, build, test, publish docs, and prep a release"
  },
  "engines": {
    "node": ">=8.9"
  },
  "dependencies": {
    "date-fns": "^2.0.0-alpha.26",
    "locutus": "^2.0.10",
    "timezone-support": "^1.8.0"
  },
  "devDependencies": {
    "ava": "1.0.0-beta.7",
    "codecov": "^3.1.0",
    "cz-conventional-changelog": "^2.1.0",
    "fast-glob": "^2.2.4",
    "gh-pages": "^2.0.0",
    "npm-run-all": "^4.1.3",
    "npm-scripts-info": "^0.3.7",
    "nyc": "^13.0.1",
    "opn-cli": "^3.1.0",
    "prettier": "^1.14.3",
    "standard-version": "^4.4.0",
    "tempy": "^0.2.1",
    "trash-cli": "^1.4.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-immutable": "^4.7.0",
    "typedoc": "^0.12.0",
    "typescript": "^3.0.3",
    "unique-temp-dir": "^1.0.0"
  },
  "ava": {
    "failFast": true,
    "files": [
      "build/main/**/*.spec.js"
    ],
    "sources": [
      "build/main/**/*.js"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "prettier": {
    "singleQuote": false,
    "trailingComma": "all"
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js"
    ]
  }
}
